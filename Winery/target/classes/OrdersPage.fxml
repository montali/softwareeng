<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>


<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="main.OrdersPage">
   <children>
      <VBox alignment="CENTER" prefHeight="400.0" prefWidth="600.0">
         <children>
            <Text strokeType="OUTSIDE" strokeWidth="0.0" text="My orders" textAlignment="CENTER">
               <font>
                  <Font name="Montserrat Light" size="24.0" />
               </font>
               <VBox.margin>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </VBox.margin>
            </Text>
            <TableView fx:id="ordersTable" prefHeight="283.0" prefWidth="580.0">
              <columns>
                <TableColumn fx:id="wineColumn" prefWidth="299.0" text="Wine" />
                  <TableColumn fx:id="yearColumn" minWidth="0.0" prefWidth="69.0" text="Year" />
                <TableColumn fx:id="qtyColumn" prefWidth="90.0" text="Q.ty" />
                  <TableColumn fx:id="statusColumn" prefWidth="117.0" text="Status" />
              </columns>
               <VBox.margin>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </VBox.margin>
            </TableView>
         </children>
      </VBox>
   </children>
</AnchorPane>
